. /lib/functions.sh
. /lib/functions/uci-defaults.sh

board_config_update

case "$(board_name)" in
	iptime,ax2004m)
		ucidef_set_compat_version "2.0"
		;;
	tplink,er605-v2)
		ucidef_set_compat_version "1.2"
		;;
	xiaomi,mi-router-cr660x)
		ucidef_set_compat_version "1.0"
		;;
	*)
		ucidef_set_compat_version "1.1"
		;;
esac

board_config_flush

exit 0
